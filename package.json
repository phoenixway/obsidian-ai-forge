{
	"name": "obsidian-ai-forge",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && npm run build:sass",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"watch:sass": "sass --watch src/styles/main.scss:styles.css --quiet",
		"build:sass": "sass src/styles/main.scss:styles.css --style=compressed --quiet",
		"test": "jest"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/date-fns": "^2.5.3",
		"@types/jest": "^29.5.14",
		"@types/node": "^16.11.6",
		"@types/pdf-parse": "^1.1.4",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.26.1",
		"@typescript-eslint/parser": "^8.26.1",
		"builtin-modules": "3.3.0",
		"date-fns": "file:latest/locale/uk",
		"esbuild": "0.17.3",
		"eslint": "^9.22.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"obsidian": "latest",
		"sass": "^1.86.3",
		"ts-jest": "^29.3.2",
		"tslib": "2.4.0",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@ricky0123/vad-web": "^0.0.24",
		"faiss-node": "^0.5.1",
		"gpt-tokenizer": "^2.9.0",
		"marked": "^15.0.7",
		"node-fetch": "^3.3.2",
		"pdf-parse": "^1.1.1",
		"uuid": "^11.1.0"
	}
}
